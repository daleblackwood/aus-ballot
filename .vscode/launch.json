    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Ingest All",
            "program": "${workspaceFolder}/ingest/index.js",
            "args": ["all"],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Ingest Electorates",
            "program": "${workspaceFolder}/ingest/index.js",
            "args": ["electorates"],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Ingest Parties",
            "program": "${workspaceFolder}/ingest/index.js",
            "args": ["parties"],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}
